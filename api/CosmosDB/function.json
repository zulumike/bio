{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "updateDocument",
      "databaseName": "biodb",
      "collectionName": "biocollection",
      "createIfNotExists": false,
      "id": "{memberid}",
      "connectionStringSetting": "dbString"
    }
  ]
}